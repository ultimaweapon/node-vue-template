{
  "name": "node-vue-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf build dist && mkdirp build dist/public",
    "build": "node src/client/build.js dist/public && tsc --project src/server --outDir build",
    "postbuild": "merge-dirs build/server dist && merge-dirs build/commons dist",
    "start": "npm run build && node dist/index.js"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "merge-dirs": "^0.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^1.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "rimraf": "^3.0.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.3",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "vue": "^2.6.11",
    "vue-functional-data-merge": "^3.1.0",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.4.0",
    "webpack": "^5.0.0-beta.18"
  }
}
